{
  "name": "<%=name%>",
  "version": "1.0.0",
  "prefix": "<%=prefix%>",
  "icons": {
<% glyphs.forEach((glyph, index) => { %>    "<%=glyph.name%>": {
      "unicode": <%=glyph.unicode[0].codePointAt(0).toString()%>,
      "hex": "<%=glyph.unicode[0].codePointAt(0).toString(16)%>",
      "character": "<%=glyph.unicode%>",
      "className": "<%=prefix%>-<%=glyph.name%>"
    }<%= index < glyphs.length - 1 ? ',' : '' %>
<% }); %>  }
}
