    "ejs": "^3.1.6",
    "glob": "^7.1.6",
    "njfs": "^1.2.5",
    "svg2ttf": "^5.1.0",
    "svgo": "^2.2.2",
    "svgpath": "^2.3.1",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.2",
    "ttf2woff2": "^4.0.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-plugin-node": "^11.1.0"
  },
  "author": "Orçun Saltık <saltikorcun@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orcunsaltik/iconism.git"
