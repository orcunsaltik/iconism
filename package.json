{
	"name": "iconism",
	"version": "1.2.3",
	"description": "A complete fonts and web assets generator from svg icon files",
	"keywords": [
		"icon",
		"icons",
		"svg",
		"ttf",
		"woff",
		"woff2",
		"eot",
		"css",
		"scss",
		"sass",
		"json",
		"html",
		"assets",
		"fonts",
		"font",
		"webfont"
	],
	"main": "lib/index.js",
	"bin": {
		"iconism": "bin/index.js"
